[Peer]
# Puppet Name: <%= @name %>
PublicKey = <%= @public_key %>
<% if @preshared_key -%>
PresharedKey = <%= @preshared_key %>
<% end -%>
AllowedIps = <%= @allowed_ips.join(',') %>
<% if @endpoint -%>
Endpoint = <%= @endpoint %>
<% end -%>
<% if @persistent_keepalive -%>
PersistentKeepalive = <%= @persistent_keepalive %>
<% end -%>
